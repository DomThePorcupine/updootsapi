updoots_web:
  build: .
  links:
    - updoots_db
  ports:
    - "3000:3001"
updoots_db:
  image: mysql
  environment:
    MYSQL_PASSWORD: npassword
    MYSQL_USER: nuser
    MYSQL_ROOT_PASSWORD: npassword
  ports:
    - "3306:3306"
  volumes:
    - /opt/mysql_data:/var/lib/mysql
    - schema.sql:/docker-entrypoint-initdb.d/
    